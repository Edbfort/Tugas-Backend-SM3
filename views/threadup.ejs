          label: "Mika Main Menu",
          prependTo: ".header",
        });

        $(".form-date input").datepicker({
          autoSize: true,
          dateFormat: "yy-mm-dd",
        });

        $('input[type="date"]').each(function () {
          $(this).attr("type", "text");
          $(this).addClass("datepicker");
        });

        $(".datepicker").datepicker({
          autoSize: true,
          dateFormat: "yy-mm-dd",
        });

        $('input[type="time"]').each(function () {
          $(this).attr("type", "text");
          $(this).addClass("time");
        });

        $(".time").timepicker({
          format: "HH:mm",
          pickSeconds: false,
          pick12HourFormat: false,
        });
        $(".level_1 > li").mouseenter(function () {
          var w = window,
            d = document,
            e = d.documentElement,
            g = d.getElementsByTagName("body")[0],
            x = w.innerWidth || e.clientWidth || g.clientWidth,
            y = w.innerHeight || e.clientHeight || g.clientHeight;

          var result = (y * 5) / 100;
          if (
            $(this).parent().height() < $(this).children(".level_2").height()
          ) {
            $(".mega-dropdown-menu").css(
              "height",
              $(this).children(".level_2").height() + result
            );
          } else {
            $(this)
              .children(".level_2")
              .css("height", $(this).parent().height() + result);
          }
        });
        $(".mega-dropdown-menu").mouseenter(function () {
          $(this).prev().css({
            "background-color": "white",
            color: "#094584",
          });
        });
        $(".mega-dropdown-menu").mouseleave(function () {
          $(this).prev().css({
            "background-color": "#094584",
            color: "#e5e5e5",
          });
        });
        $("div#menu > ul > li > .parent").mouseenter(function () {
          $(this).css({
            "background-color": "white",
            color: "#094584",
          });
        });
        $("div#menu > ul > li > .parent").mouseleave(function () {
          $(this).css({
            "background-color": "#094584",
            color: "#e5e5e5",
          });
        });
        $("div#menu ul.menu_level_1 > li").mouseenter(function () {
          var siblingsLi = $(this).parent().children();
          siblingsLi.removeClass("selected-li-level-1");
          $(this).parents().eq(1).next().children().hide();
          if ($(this).children("a").hasClass("parent")) {
            $("ul" + $(this).attr("id")).show();
            $(this).addClass("selected-li-level-1");
          }
        });
      });
      $(window).resize(megaMenuChange);
