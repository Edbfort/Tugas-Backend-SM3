      <div class="wrapper">
        <div class="identitas-login-user">
          <strong> <%= user.name %>#<%= user.code %> </strong>
        </div>
        <div class="mika-box detail-berita-login">
          <h3>Upload Thread</h3>
          <div class="content-container">
            <div class="content-container">
              <p></p>
              <form action="/api/thread/upload" method="post">
                <input
                  type="text"
                  hidden
                  id="name"
                  name="name"
                  value="<%= user.name %>"
                />
                <input
                  type="text"
                  hidden
                  id="code"
                  name="code"
                  value="<%= user.code %>"
                />
                <br />
                <label>Title</label><br />
                <input
                  type="text"
                  name="title"
                  id="title"
                  placeholder="Title"
                  size="60px"
                /><br /><br />
                <label>Kategori</label><br />
                <input type="radio" id="sfw" name="kategori" value="SFW" />
                <label for="sfw">SFW</label>
                <!-- <br> -->
                <input type="radio" id="nsfw" name="kategori" value="NSFW" />
                <label for="nsfw">NSFW</label><br /><br />
                <label>Thread Content</label><br />
                <textarea
                  type="text"
                  name="content"
                  id="content"
                  placeholder="Content"
                ></textarea
                ><br />
                <br />
                <input type="submit" class="mika-button" value="Submit" />
              </form>
              <p></p>
            </div>
          </div>
        </div>
      </div>
