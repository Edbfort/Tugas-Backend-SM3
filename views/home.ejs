                <li>
                  <a href="<%=nextp%>page=<%= page.current+1 %>"
                    ><%= page.current+1 %></a
                  >
                </li>
                <% } %>%> <% if (page.total-page.current==1&&page.current>3) {
                %>
                <li class="active">
                  <span
                    ><%= page.total-1 %>
                    <span class="sr-only">(current)</span></span
                  >
                </li>
                <% } else if
                ([0,3,2].indexOf(page.total-page.current)!=-1&&page.total>3&&page.total!=4)
                { %>
                <li><a href="<%=nextp%>page=2"><%= page.total-1 %></a></li>
                <% } else if(page.total-page.current>=2&&page.total>3) { %>
                <li class="disabled"><span>…</span></li>
                <% } %> <% if
                (page.current==page.total&&[1,2,3].indexOf(page.current)==-1) {
                %>
                <li class="active">
                  <span
                    ><%= page.total %>
                    <span class="sr-only">(current)</span></span
