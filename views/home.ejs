                    <span> </span>
                    <a
                      target="_blank"
                      class="read-more"
                      href="<%= thread.links_ %>"
                      >baca selengkapnya</a
                    >
                  </p>
                </div>
              </div>
              <% })} %>
            </div>
            <div class="news-pagination">
              <ul class="pagination">
                <% if (page.current==1||page.current==0) { %>
                <li class="prev disabled"><span>«</span></li>
                <% } else { %>
                <li class="prev">
                  <a href="<%=nextp%>page=<%= page.current-1 %>" rel="prev">«</a>
                </li>
                <% } %> <% if (page.current==1) { %>
