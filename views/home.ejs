                <li class="active">
                  <span>3 <span class="sr-only">(current)</span></span>
                </li>
                <% } else if([1,2,4].indexOf(page.current)!=-1&&page.total>3){
                %>
                <li><a href="<%=nextp%>page=3">3</a></li>
                <% } else if(page.current>=4&&page.total-page.current>=2){ %>
                <li>
                  <a href="<%=nextp%>page=<%= page.current-1 %>"
                    ><%= page.current-1 %></a
                  >
                </li>
                <% } %>%> <% if (page.current>=4&&page.total-page.current>=3) {
                %>
                <li class="active">
                  <span
                    ><%= page.current %>
                    <span class="sr-only">(current)</span></span
                  >
                </li>
                <% }%> <% if (page.total-page.current==2&&page.current>3) { %>
                <li class="active">
                  <span
                    ><%= page.total-2 %>
                    <span class="sr-only">(current)</span></span
                  >
                </li>
                <% } else
